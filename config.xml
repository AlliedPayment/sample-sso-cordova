<?xml version='1.0' encoding='utf-8'?>
<widget id="com.alliedpayment.sso" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Allied SSO Demo</name>
    <description>An application used to demo / prototype Allied Payment's Single Sign On.
    </description>
    <author email="nate.ross@alliedpayment.com" href="http://alliedpayment.com">Allied Payment Network Dev Team
    </author>
    <access origin="*" />
    <allow-navigation href="*" />
    <content src="index.html" />
    <engine name="android" spec="^6.2.3" />
    <plugin name="cordova-plugin-camera" spec="^2.3.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
